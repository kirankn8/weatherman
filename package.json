{
  "name": "weatherman",
  "displayName": "WeatherMan",
  "description": "Provides intelligent weather forecast",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:weatherman.forecast"
  ],
  "main": "dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "weatherman.forecast",
        "title": "WeatherMan"
      }
    ]
  },
  "scripts": {
    "start": "webpack serve",
    "build": "rimraf ./dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "build:exp": "webpack --config webpack.config.js --color",
    "watch": "webpack --watch",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^5.3.2",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.0.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "eslint-plugin-react": "^7.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.2.0"
  }
}
